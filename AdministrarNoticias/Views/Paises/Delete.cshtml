@model AdministrarNoticias.Paise

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar país</h1>

<h4>¿Estás seguro que quieres eliminar eso?</h4>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre del país
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombrePais)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="PaisId" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
