@model AdministrarNoticias.Fuente

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar fuente</h1>

<h4>¿Estas seguro que quieres eliminar eso?</h4>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Nombre de la fuente
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreFuente)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="FuenteId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
